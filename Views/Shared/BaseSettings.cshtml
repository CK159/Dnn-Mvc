@inherits DotNetNuke.Web.Mvc.Framework.DnnWebViewPage <SampleMVC.Modules.SampleMVC.Mvc.MvcSettingsModel>

@using DotNetNuke.Web.Mvc.Helpers

<h2 id="dnnSitePanel-BasicSettings" class="dnnFormSectionHead"><a href="" class="dnnSectionExpanded">@Dnn.LocalizeString("BasicSettings")</a></h2>
<fieldset>
    <div class="dnnFormItem">
        <div class="dnnLabel" style="position: relative;">
            @Html.LabelFor(m => Model.Settings.Route, new { @class = "col-md-2 control-label" })
        </div>
        @Html.DropDownListFor(m => m.Settings.Route, Model.Routes, "—Select Route—")
    </div>
</fieldset>

